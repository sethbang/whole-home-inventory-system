
[req]
default_bits = 4096
prompt = no
default_md = sha384
req_extensions = v3_req
distinguished_name = dn

[dn]
C = US
ST = Colorado
L = Denver
O = WHIS Development
OU = Development
CN = localhost
emailAddress = dev@whis.local

[v3_req]
basicConstraints = CA:FALSE
keyUsage = digitalSignature, keyEncipherment
extendedKeyUsage = serverAuth, clientAuth
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost
DNS.2 = *.localhost
DNS.3 = whis.local
DNS.4 = *.whis.local
DNS.5 = nas.local
DNS.6 = *.nas.local
IP.1 = 127.0.0.1
IP.2 = ::1
IP.3 = 192.168.1.15
IP.4 = 172.17.0.1
IP.5 = 172.18.0.1
